{
  "version": 2,
  "builds": [
    {
      "src": "src",
      "use": "@vercel/node"
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/convert",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.html"
    }
  ]
}
